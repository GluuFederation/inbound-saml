{
  "name": "inbound-saml",
  "version": "0.10.0",
  "description": "Gluu inbound-saml module",
  "repository": {
    "type": "git",
    "url": "git@github.com:GluuFederation/inbound-saml.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/sp-proxy/src/frameworks-drivers/main/server.js",
    "test": "jest --no-cache --verbose",
    "test:watch": "npm test -- --watch",
    "test:all": "npm test",
    "test:integration": "npm test -- --config jest-integration-config.js",
    "test:integration:docker": "docker-compose -f docker-compose-test.yml up -d && npm test -- --config jest-integration-config.js && docker-compose -f docker-compose-test.yml down",
    "test:coverage": "jest --ci --coverage",
    "test:behaviour": "cucumber-js packages/get-saml-metadata packages/sp-proxy -p default --exit",
    "test:staged": "npm test -- --findRelatedTests",
    "prepare": "husky install",
    "build": "tsc && tsc-alias",
    "dev": "nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register src/sp-proxy/src/frameworks-drivers/main/server.ts' -e ts",
    "dev:debug": "nodemon --inspect 'src/' --exec 'ts-node -r tsconfig-paths/register src/sp-proxy/src/frameworks-drivers/main/server.ts' -e ts"
  },
  "author": {
    "name": "Christian Eland",
    "email": "chris@gluu.org"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@cucumber/cucumber": "^7.2.1",
    "@types/chai": "^4.2.21",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.3.1",
    "@types/passport": "^1.0.7",
    "@types/passport-saml": "^1.1.3",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@types/xml2js": "^0.4.9",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.30.0",
    "chai": "^4.3.4",
    "codecov": "^3.8.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "^7.0.1",
    "intermock": "^0.2.5",
    "jest": "^27.0.6",
    "lerna": "^4.0.0",
    "lint-staged": "^11.1.1",
    "nock": "^13.1.1",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "standard-version": "^9.3.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "tsc-alias": "^1.3.8",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "fast-xml-parser": "^3.19.0",
    "mongodb": "^4.1.0",
    "passport": "^0.4.1",
    "passport-saml": "^3.1.2",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "xml2js": "^0.4.23"
  }
}
