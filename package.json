{
  "name": "inbound-saml",
  "version": "0.17.5",
  "description": "Gluu inbound-saml module",
  "repository": {
    "type": "git",
    "url": "git@github.com:GluuFederation/inbound-saml.git"
  },
  "workspaces": [
    "packages/!(testdata)/**"
  ],
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=prod node ./dist/sp-proxy/src/frameworks-drivers/main/server.js",
    "test": "jest --no-cache --verbose",
    "test:watch": "npm test -- --watch",
    "test:unit": "yarn test --config jest-unit-config.js",
    "test:integration": "yarn test --config jest-integration-config.js --runInBand",
    "test:all": "yarn test:unit && yarn test:integration",
    "test:integration:docker": "docker-compose -f docker-compose-test.yml up -d && yarn test:integration && docker-compose -f docker-compose-test.yml down",
    "test:coverage": "jest --ci --coverage",
    "test:behaviour": "NODE_ENV=test INBOUND_SAML_USE_TLS=true cucumber-js packages/get-saml-metadata packages/sp-proxy -p default --exit",
    "test:staged:unit": "yarn test:unit --no-cache --findRelatedTests",
    "test:staged:integration": "yarn test:integration --no-cache --findRelatedTests",
    "prepare": "husky install",
    "build": "tsc && tsc-alias",
    "dev": "nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register packages/sp-proxy/src/frameworks-drivers/main/server.ts' -e ts",
    "dev:debug": "nodemon --inspect 'src/' --exec 'ts-node -r tsconfig-paths/register packages/sp-proxy/src/frameworks-drivers/main/server.ts' -e ts",
    "postinstall": "husky install",
    "prepublishOnly": "psinst --disable",
    "postpublish": "psinst --enable",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "author": {
    "name": "Christian Eland",
    "email": "chris@gluu.org"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@cucumber/cucumber": "^7.2.1",
    "@faker-js/faker": "^7.6.0",
    "@types/chai": "^4.2.21",
    "@types/express": "^4.17.14",
    "@types/jest": "29",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/node": "^18.7.23",
    "@types/passport": "^1.0.11",
    "@types/passport-saml": "^1.1.3",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.2",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "chai": "^4.3.4",
    "codecov": "^3.8.3",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^26.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "intermock": "^0.2.5",
    "jest": "29",
    "lerna": "^5.5.2",
    "lint-staged": "^13.1.0",
    "nock": "^13.2.9",
    "nodemon": "2.0.20",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "standard-version": "^9.3.1",
    "supertest": "^6.3.3",
    "ts-jest": "29",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "axios": "0.27.2",
    "express": "^4.18.2",
    "fast-xml-parser": "3",
    "jsonwebtoken": "^9.0.0",
    "passport": "^0.6.0",
    "passport-saml": "^3.2.4",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "xml2js": "^0.4.23"
  },
  "packageManager": "yarn@3.2.3",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
